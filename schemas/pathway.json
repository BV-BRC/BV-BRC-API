{
  "$schema": "http://json-schema.org/draft-04/hyper-schema#",
  "title": "pathway",
  "type": "object",
  "pathStart": "/pathway",
  "links": [
    {
      "rel": "self",
      "title": "Get a pathway instance",
      "method": "GET",
      "template": "default",
      "href": "/pathway/{id}",
      "targetSchema": {
        "$ref": "#"
      }
    },
    {
      "rel": "query",
      "title": "Query pathway instances.",
      "method": "GET",
      "href": "/pathway",
      "template": "default-list",
      "targetSchema": {
        "type": "array",
        "items": {
          "$ref": "#"
        }
      }
    },
    {
      "rel": "create",
      "title": "Create pathway(s).  Throw error if pathway already exists.",
      "encType": "application/json",
      "method": "POST",
      "href": "/pathway",
      "schema": {
        "oneOf": [
          {
            "$ref": "#"
          },
          {
            "type": "array",
            "items": {
              "$ref": "#"
            }
          }
        ]
      },
      "targetSchema": {
        "type": "array",
        "items": {
          "oneOf": [
            {
              "$ref": "#"
            },
            {
              "type": "array",
              "items": {
                "$ref": "#"
              }
            }
          ]
        }
      }
    },
    {
      "rel": "update",
      "title": "Update pathway. Partial Update OK. Throw error if pathway does not exist",
      "encType": "application/json",
      "method": "POST",
      "href": "/pathway/{id}",
      "schema": {
        "$ref": "#"
      },
      "targetSchema": {
        "$ref": "#"
      }
    },
    {
      "rel": "create",
      "title": "Create pathway. Overwrite it if it already exists.",
      "encType": "application/json",
      "method": "PUT",
      "href": "/pathway/{id}",
      "schema": {
        "$ref": "#"
      },
      "targetSchema": {
        "type": "null"
      }
    },
    {
      "rel": "delete",
      "title": "Delete a pathway",
      "method": "DELETE",
      "href": "/pathway/{id}",
      "schema": {
        "$ref": "#"
      },
      "targetSchema": {
        "type": "null"
      }
    },
    {
      "rel": "delete",
      "title": "Delete one ore more pathway by query",
      "method": "DELETE",
      "href": "/pathway/{,query*}",
      "schema": {
        "$ref": "#"
      },
      "targetSchema": {
        "type": "null"
      }
    },
    {
      "rel": "rpc",
      "title": "JSON-RPC Endpoint for the pathway Model.  Parameters and Response schema as per the associated link relation.",
      "encType": "application/jsonrpc+json",
      "mediaType": "application/jsonrpc+json",
      "method": "POST",
      "href": "/pathway"
    }
  ],
  "uniqueKey": "id",
  "properties": {
    "_version_": {
      "indexed": true,
      "type": "integer"
    },
    "accession": {
      "indexed": true,
      "type": "string"
    },
    "alt_locus_tag": {
      "indexed": true,
      "type": "string"
    },
    "annotation": {
      "indexed": true,
      "type": "string"
    },
    "annotation_sort": {
      "indexed": true,
      "type": "string",
      "transient": true
    },
    "date_inserted": {
      "indexed": true,
      "type": "string",
      "format": "datetime"
    },
    "date_modified": {
      "indexed": true,
      "type": "string",
      "format": "datetime"
    },
    "ec_description": {
      "indexed": true,
      "type": "string"
    },
    "ec_number": {
      "indexed": true,
      "type": "string"
    },
    "feature_id": {
      "indexed": true,
      "type": "string"
    },
    "gene": {
      "indexed": true,
      "type": "string"
    },
    "genome_ec": {
      "indexed": true,
      "type": "string"
    },
    "genome_id": {
      "indexed": true,
      "type": "string"
    },
    "genome_name": {
      "indexed": true,
      "type": "string"
    },
    "id": {
      "indexed": true,
      "type": "string",
      "unique": true
    },
    "owner": {
      "indexed": true,
      "type": "string"
    },
    "pathway_class": {
      "indexed": true,
      "type": "string"
    },
    "pathway_ec": {
      "indexed": true,
      "type": "string"
    },
    "pathway_id": {
      "indexed": true,
      "type": "string"
    },
    "pathway_name": {
      "indexed": true,
      "type": "string"
    },
    "product": {
      "indexed": true,
      "type": "string"
    },
    "public": {
      "indexed": true,
      "type": "string"
    },
    "sequence_id": {
      "indexed": true,
      "type": "string"
    },
    "taxon_id": {
      "indexed": true,
      "type": "integer"
    },
    "text": {
      "indexed": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true,
      "transient": true
    },
    "user_read": {
      "indexed": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "user_write": {
      "indexed": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    }
  },
  "required": [
    "id"
  ]
}
